    "mysql2": "^3.11.5",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^20.17.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "nodemon": "^3.1.7",
    "postcss": "^8",
